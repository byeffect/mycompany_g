MODULE OperationOrder;

REQUIRE OperationLot;

NAMESPACE Manufacturing;

@defineOperation(order, 'Заказ', ordered);
@defineOperationLot(order, 'заказа');

newOperationOrder 'Заказ' (Lot l) {
    NEW o = OperationOrder {
        lot(o) <- l;
    }
}

EXTEND FORM lot
    PROPERTIES(l) newOperationOrder DRAW o TOOLBAR
;